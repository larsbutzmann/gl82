header.navbar-inverse.navbar-default.navbar-fixed-top(role="navigation")
  div.container
    div.navbar-header
      button.navbar-toggle(type="button", data-toggle="collapse", data-target=".navbar-ex5-collapse")
        span.sr-only Toggle navigation
        span.icon-bar
        span.icon-bar
        span.icon-bar
      a.navbar-brand(href="/") GL82 IfG Erfurt
    div.collapse.navbar-collapse.navbar-ex5-collapse
      ul.nav.navbar-nav
        - var obj = { 'studium':'Studium', 'seminargruppe':'Seminargruppe', 'dozenten':'Dozenten', 'seminartreffen': 'Seminartreffen', 'maennertag': 'Männertag', 'brandenburgtag': 'Brandenburg-Tag', 'gaestebuch': 'Gästebuch' }
          - each val, key in obj
            - if (key == "studium")
              li.dropdown
                a.dropdown-toggle(href="#", data-toggle="dropdown") Studium
                ul.dropdown-menu
                  li
                    a(href="#") Immatrikulation
                    a(href="#") Praktika
                    a(href="#") Seminare
                    a(href="#") Weihnachtsfeiern
                    a(href="#") Frauentag
                    a(href="#") Männertag
                    a(href="#") Fasching
                    a(href="#") Silvester
            - else
              li
                a(id="#{key}", href='#{key}') #{val}
